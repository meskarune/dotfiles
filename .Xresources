!stjerm.key: f1
!stjerm.opacity: 60
!stjerm.foreground: #ffffff
!stjerm.width: 100%
!stjerm.height: 60%
!stjerm.fixedY: 26
!stjerm.scrollbar: right
!stjerm.font: monaco 10

URxvt.scrollBar: none
URxvt.font: xft: Dejavu Sans Mono for Powerline:size=9
URxvt.perl-ext-common:default,matcher,vtwheel
!URxvt.depth: 32
!URxvt.background: [40]#000000
URxvt.foreground: #ffffff

URxvt.transparent: true
! URxvt.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt.shading: 60

URxvt.urlLauncher: google-chrome-stable
URxvt.matcher.button: 3
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.colorUL: #4682B4

